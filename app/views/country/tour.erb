<h3><%= @tour.tour_name %></h3><br>
<% @tour.images.each do |image| %>
	<img src="<%= image.url %>" height="600" width="850"><br>
<% end %>
<%= @tour.content %><br>
<p>The time you might want to spend in this place is around <%= @tour.time_needed %> hours.</p>
<p>$<%= @tour.price %></p>
<a id="<%= @tour.id %>" href="/users/<%=session[:user_id]%>/wishlist/<%= @tour.id %>" class="add_to_wishlist" data="<%= @tour.tour_name %>">Add this item to wish list</a>

